{
  "name": "express-ts-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon --exec 'ts-node src/index.ts'",
    "start:inspect": "nodemon --exec node -r ts-node/register --inspect=0.0.0.0:9229 src/index.ts",
    "console": "node -r ts-node/register --experimental-repl-await console.ts",
    "console:inspect": "node -r ts-node/register --inspect=0.0.0.0:9339 --experimental-repl-await console.ts",
    "build": "tsc --project tsconfig.json",
    "serve": "TS_NODE_BASEURL=dist node -r tsconfig-paths/register dist/src/index.js",
    "clean": "npm run lint:fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:verbose": "jest --verbose",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@swc/core": "^1.3.58",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eqeqeq-fix": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.11",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "filestack-js": "^3.38.5",
    "fs-extra": "^11.3.0",
    "http-errors": "^2.0.0",
    "superagent": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
